    <sql>SELECT&#xd;&#xa;CicloFundo2015.IdProjeto as IdProjeto,&#xd;&#xa;CicloFundo2015.IdFundo as IdFundo2015&#xd;&#xa;&#xd;&#xa;&#xd;&#xa; FROM CicloFundo as CicloFundo2015&#xd;&#xa;						 INNER JOIN AccountingCalendar  on &#x28;AccountingCalendar.Project &#x3d; CicloFundo2015.IdProjeto AND AccountingCalendar.FiscalYear &#x3d; year&#x28;getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;						left join CicloFundo as CicloFundo2016 on &#x28;CicloFundo2015.IdProjeto &#x3d; CicloFundo2016.IdProjeto &#xd;&#xa;						and CicloFundo2015.IdFundo &#x3d; CicloFundo2016.IdFundo and CicloFundo2016.AnoFiscal &#x3d;year&#x28;getdate&#x28;&#x29;&#x29;&#x29; --add ano fiscal para planejamento&#xd;&#xa;					&#xd;&#xa;WHERE CicloFundo2015.AnoFiscal &#x3d;year&#x28;getdate&#x28;&#x29;&#x29; -1 -- ano fiscal atual&#xd;&#xa;AND CicloFundo2016.IdFundo IS NULL&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;ORDER BY CicloFundo2015.IdFundo &#xd;&#xa;</sql>
